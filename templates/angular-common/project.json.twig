{
  "name": "appwrite-common",
  "$schema": "..\\..\\..\\node_modules\\nx\\schemas\\project-schema.json",
  "sourceRoot": "libs/appwrite/common/src",
  "projectType": "library",
  "targets": {
    "lint": {
      "executor": "@nx/linter:eslint",
      "options": {
        "lintFilePatterns": ["libs/appwrite/common/**/*.ts"]
      },
      "outputs": ["{options.outputFile}"]
    },
    "test": {
      "executor": "@nx/jest:jest",
      "outputs": ["coverage/libs/appwrite/common"],
      "options": {
        "jestConfig": "libs/appwrite/common/jest.config.ts",
        "passWithNoTests": true
      }
    },
    "build": {
      "executor": "@nx/js:tsc",
      "outputs": ["{options.outputPath}"],
      "options": {
        "outputPath": "dist/libs/appwrite/common",
        "tsConfig": "libs/appwrite/common/tsconfig.lib.json",
        "packageJson": "libs/appwrite/common/package.json",
        "main": "libs/appwrite/common/src/index.ts",
        "assets": ["libs/appwrite/common/*.md"]
      }
    }
  },
  "tags": []
}

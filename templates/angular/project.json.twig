{
  "name": "appwrite-angular",
  "$schema": "..\\..\\..\\node_modules\\nx\\schemas\\project-schema.json",
  "projectType": "library",
  "sourceRoot": "libs/appwrite/angular/src",
  "prefix": "mm-mono",
  "targets": {
    "build": {
      "executor": "@nx/angular:package",
      "outputs": ["{workspaceRoot}/dist/libs/appwrite/angular"],
      "options": {
        "project": "libs/appwrite/angular/ng-package.json"
      },
      "configurations": {
        "production": {
          "tsConfig": "libs/appwrite/angular/tsconfig.lib.prod.json"
        },
        "development": {
          "tsConfig": "libs/appwrite/angular/tsconfig.lib.json"
        }
      },
      "defaultConfiguration": "production"
    },
    "test": {
      "executor": "@nx/jest:jest",
      "outputs": ["{workspaceRoot}/coverage/libs/appwrite/angular"],
      "options": {
        "jestConfig": "libs/appwrite/angular/jest.config.ts",
        "passWithNoTests": true
      }
    },
    "lint": {
      "executor": "@nx/linter:eslint",
      "options": {
        "lintFilePatterns": ["libs/appwrite/angular/src/**/*.ts", "libs/appwrite/angular/src/**/*.html"]
      },
      "outputs": ["{options.outputFile}"]
    }
  },
  "tags": []
}
